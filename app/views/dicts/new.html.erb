<h1>Add a new dictionary</h1>
<%= form_for(@dict, :action => resource(:dicts) ) do %>
 
  <p>
    <%= text_field :name, :label => "Name" %>
  </p>
 
  <p>
    <%= text_field :description, :label => "Description"  %>
  </p>
 
  <p>
    <%= submit "Create" %>
  </p>
<% end =%>
 
<%= link_to 'Back', resource(:dicts) %>
